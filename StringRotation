package practice;

public class StringRotation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="apple";
		StringBuffer sb=new StringBuffer();
		sb.append(s);
		sb.append(s);
		String s3=new String(sb);
		String s4="leapp";
		int n=s.length();
		String arr[]=new String[n];
		for(int i=0;i<s.length();i++)
		{
			/*To check if 2 strings are totation of each other
			 * if(s4.equals(s3.substring(i,i+n)))/
				System.out.println("YES"); or count++
			else
				System.out.println("NO");*/
			//2.TO STORE AND PRINT THE ARRAY CONTAINING STRING ROATIONS
			arr[i]=s3.substring(i,i+n);
			System.out.println(arr[i]);
			
		}
		
		

	}

}
